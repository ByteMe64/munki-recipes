Description: Downloads the latest version of Gyroflow and imports it into Munki.
Identifier: com.github.ByteMe64.munki.Gyroflow
ParentRecipe: com.github.ByteMe64.download.Gyroflow

Input:
  NAME: Gyroflow
  MUNKI_REPO_SUBDIR: apps/gyroflow
  pkginfo:
    catalogs:
      - testing
    description: Gyroflow is a post-processing video stabilization software based on logged motion data.
    display_name: Gyroflow
    name: "%NAME%"
    unattended_install: true

Process:
  - Processor: MunkiImporter
    Arguments:
      pkg_path: "%pathname%"
      repo_subdirectory: "%MUNKI_REPO_SUBDIR%"